/**
 * Definition for a binary tree node.
 * struct TreeNode {
 *     int val;
 *     TreeNode *left;
 *     TreeNode *right;
 *     TreeNode(int x) : val(x), left(NULL), right(NULL) {}
 * };
 */
class Solution {
public:
    vector<int> ans;
    vector<int> inorderTraversal(TreeNode* root) {
        f(root);
        return ans;
    }
    void f(TreeNode *cur){
        if(cur==NULL) return;
        f(cur->left);
        ans.push_back(cur->val);
        f(cur->right);
    }
};
